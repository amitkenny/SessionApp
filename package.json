{
  "name": "sessionApp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "browserify": "browserify ./client/main.js -o ./public/javascripts/bundle.js",
    "parcelify": "parcelify ./client/main.js -c ./public/stylesheets/bundle.css --watch --loglevel 'info'  ",
    "watch": "watchify ./client/main.js -o ./public/javascripts/bundle.js -v"
  },
  "dependencies": {
    "body-parser": "~1.8.1",
    "bootstrap": "^3.3.2",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "express-session": "^1.10.3",
    "hbs": "~2.7.0",
    "jquery": "^2.1.3",
    "morgan": "~1.3.0",
    "serve-favicon": "~2.1.3"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "parcelify": "^0.12.5",
    "watchify": "^2.4.0"
  }
}
